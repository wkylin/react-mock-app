{
  //环境定义了预定义的全局变量。更多在官网查看
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "amd": true,
    "es6": true,
    "mocha": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/jsx-filename-extension": [
      "warn",
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "react/prop-types": 0
    //防止在React组件定义中丢失props验证
  },
  "extends": "airbnb",
  "parser": "babel-eslint"
}
