{
  "name": "react-mock-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:dev": "env-cmd -f .env.development react-app-rewired start",
    "start:test": "env-cmd -f .env.test react-app-rewired start",
    "start:mockServer": "concurrently \"yarn run start:mock\" \"yarn run mockServer\"",
    "build:buildServer": "yarn run build && yarn run buildServer",
    "build": "env-cmd -f .env.production react-app-rewired build --profile",
    "buildServer": "serve -s build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "start:mock": "env-cmd -f .env.mock react-app-rewired start",
    "mockServer": "concurrently \"nodemon ./mock/routes/index.js\" \"cd mock && node app.js\"",
    "mock": "cd mock && node app.js"
  },
  "dependencies": {
    "antd": "^3.24.0",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "customize-cra": "^0.8.0",
    "js-cookie": "^2.2.1",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "qs": "^6.9.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-loadable": "^5.5.0",
    "react-motion": "^0.5.2",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "redux": "^4.0.4",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "babel-plugin-import": "^1.12.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.3",
    "env-cmd": "^10.0.1",
    "express": "^4.17.1",
    "http-proxy-middleware": "^0.20.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.3",
    "nodemon": "^1.19.3",
    "prettier": "^1.19.1",
    "react-app-rewired": "^2.1.3",
    "serve": "^11.2.0",
    "terser-webpack-plugin": "^2.2.1",
    "uglifyjs-webpack-plugin": "^2.2.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,less,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "pre-commit": [
    "lint"
  ]
}
